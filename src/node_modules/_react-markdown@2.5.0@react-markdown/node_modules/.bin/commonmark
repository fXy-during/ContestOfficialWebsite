#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_commonmark@0.24.0@commonmark/bin/commonmark" "$@"
  ret=$?
else 
  node  "$basedir/../../../_commonmark@0.24.0@commonmark/bin/commonmark" "$@"
  ret=$?
fi
exit $ret
